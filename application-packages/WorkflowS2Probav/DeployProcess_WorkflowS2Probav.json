{
	"processDescription": {
		"process": {
			"id": "WorkflowS2ProbaV",
			"title": "Workflow",
			"abstract": "",
			"keywords": [],
			"inputs": [{
				"id": "image-s2",
				"title": "Input Image",
				"formats": [{
					"mimeType": "application/zip",
					"default": true
				}],
				"minOccurs": "1",
				"maxOccurs": "2",
				"additionalParameters": [{
					"role": "http://www.opengis.net/eoc/applicationContext/inputMetadata",
					"parameters": [{
						"name": "EOImage",
						"values": ["true"]
					}]
				}]
			},
			{
				"id": "image-probav",
				"title": "Input Image",
				"formats": [{
					"mimeType": "application/zip",
					"default": true
				},
				{
					"mimeType": "application/x-hdf"
				},
				{
					"mimeType": "application/x-binary"
				}],
				"minOccurs": "1",
				"maxOccurs": "2",
				"additionalParameters": [{
					"role": "http://www.opengis.net/eoc/applicationContext/inputMetadata",
					"parameters": [{
						"name": "EOImage",
						"values": ["true"]
					}]
				}]
			}],
			"outputs": [{
				"id": "output",
				"title": "Workflow SFS Image",
				"formats": [{
					"mimeType": "application/zip",
					"default": true
				}]
			}]
		},
		"processVersion": "1.0.0",
		"jobControlOptions": ["async-execute"],
		"outputTransmission": ["reference"]
	},
	"executionUnit": [{
		"unit": {
			"cwlVersion": "v1.0",
			"class": "Workflow",
			"requirements": [{
				"class": "StepInputExpressionRequirement"
			},
			{
				"class": "MultipleInputFeatureRequirement"
			}],
			"inputs": {
				"image-s2": "File[]",
				"image-probav": "File[]"
			},
			"outputs": {
				"classout": {
					"type": "File",
					"outputSource": "sfs/output"
				}
			},
			"steps": {
				"stacker_s2": {
					"run": "StackerFromCRIM.cwl",
					"in": {
						"files": "image-s2"
					},
					"out": ["output"]
				},
				"stacker_probav": {
					"run": "StackerFromCRIM.cwl",
					"in": {
						"files": "image-probav"
					},
					"out": ["output"]
				},
				"stack_creation": {
					"run": "StackerFromCRIM.cwl",
					"in": {
						"files": ["stacker_s2/output",
						"stacker_probav/output"]
					},
					"out": ["output"]
				},
				"sfs": {
					"run": "SFSFromCRIM.cwl",
					"in": {
						"source_product": "stack_creation/output"
					},
					"out": ["output"]
				}
			}
		}
	}],
	"deploymentProfileName": "http://www.opengis.net/profiles/eoc/workflow"
}